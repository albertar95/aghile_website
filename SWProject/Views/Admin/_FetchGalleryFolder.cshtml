@model string

<div class="col-md-12" style="direction:rtl;float:right;">
    <div class="row">
        <div class="col-md-12">
            <div class="content-box-large">
                <div class="panel-heading">
                    <div class="panel-title" style="text-align:right;"></div>
                </div>
                <div class="panel-body" style="text-align:right;max-height:400px;">
                        <div class="col-lg-12">
                            @for (int i = 0; i < Model.Split(',').Length; i = i + 3)
                            {
                                <div class="rowgallery">
                                    @foreach (var itemm in Model.Split(',').Skip(i).Take(3))
                                    {

                                    <div class="col-md-4" style="padding:5px;">
                                        <img src="@itemm" width="100" height="100" />
                                    </div>
                                    }
                                </div>
                            }
                        </div>
                </div>
            </div>
        </div>
    </div>
</div>
